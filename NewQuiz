<SCRIPT LANGUAGE="JavaScript">
var Answer = new Array;
var done = new Array;
var YourChoice = new Array;


var score = 0;
var put = 0;
var remove = 0;
var sus = 0;
Answer[1] = "n2";
Answer[2] = "a";
Answer[3] = 110101011110;
Answer[4] = "a";
Answer[5] = "5n";
Answer[6] = "3n";
Answer[7] = "Half Adder";
Answer[8] = "c";
Answer[9] = "a";
Answer[10] = 15;
Answer[11] = "Transport";
Answer[12] = "DELETE";
Answer[13] = "Cookie";
Answer[14] = 1;
Answer[15] = "Media Access Control Address";

function Engine(question, answer) 
{
	YourChoice[question]=answer;
  
}

function Score()
{
	
	var answerText = "";
	for(i=1;i<=15;i++)
	{
  		if(Answer[i]==YourChoice[i])
  		{
			score++;
  		}
	}
	answerText=answerText+"Score : "+score+"\n";
	var percentage = score/15*100;
	answerText=answerText+"Total Percentage : "+percentage+"%\n"
	score = 0;
  
	alert(answerText);
}

   function Timer() {
    
    myVar = setTimeout(Score, 4500);
    
}

</script>

